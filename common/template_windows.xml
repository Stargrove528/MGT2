<root>
	<template name="windowtitlebar" merge="join">
		<genericcontrol>
			<anchored height="40" />
			<parameters>
				<texty>-2</texty>
			</parameters>
		</genericcontrol>
	</template>

	<!-- DESKTOP PANELS -->

	<!-- Frame: chatbox -->
	<template name="resize_chatbox">
		<resize />
	</template>
	<template name="anchor_content_chatbox_top">
		<genericcontrol name="contentanchor">
			<anchored height="0">
				<top offset="23" />
				<left offset="21" />
				<right offset="-27" />
			</anchored>
			<disabled />
		</genericcontrol>
	</template>
	<template name="anchor_content_chatbox_bottom">
		<genericcontrol name="bottomanchor">
			<anchored to="contentanchor" height="30">
				<bottom parent="" />
				<left />
				<right />
			</anchored>
			<disabled />
		</genericcontrol>
	</template>
	
