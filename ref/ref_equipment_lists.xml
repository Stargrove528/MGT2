<root>
	<!-- Weapons table -->
	<windowclass name="reference_weaponstable" copy="reference_groupindex_labeled_window">
		<sizelimits>
			<minimum width="650" height="650" />
		</sizelimits>
		<sheetdata>
			<list_content_labeled_framed_groupbox name="grouplist">
				<class>reference_weaponstablegroup</class>
				<sortby><control>tl</control></sortby>
				<sortby><control>name</control></sortby>
			</list_content_labeled_framed_groupbox>

			<spacer_list_content_link_label_left />
			<label_list_content_string_noframe_left name="headername">
				<anchored width="220" />
				<static textres="ref_label_listname" />
			</label_list_content_string_noframe_left>
			<label_list_content_string_noframe_left name="headertl">
				<anchored width="50" />
				<static textres="ref_label_listtl" />
				<center />
			</label_list_content_string_noframe_left>
			<label_list_content_string_noframe_left name="headerdamage">
				<anchored width="100" />
				<static textres="ref_label_listdamage" />
				<center />
			</label_list_content_string_noframe_left>
			<label_list_content_number_left name="headermass">
				<anchored width="100" />
				<static textres="ref_label_listmass" />
			</label_list_content_number_left>
			<label_list_content_string_noframe_left name="headercost">
				<anchored width="100" />
				<static textres="ref_label_listcost" />
				<center />
			</label_list_content_string_noframe_left>
		</sheetdata>
	</windowclass>
	<windowclass name="reference_weaponstablegroup" copy="reference_groupindex_group">
		<sheetdata>
			<list_content_groupitems name="list">
				<datasource>.equipment</datasource>
				<class>reference_weaponstablegroupitem</class>
			</list_content_groupitems>
		</sheetdata>
	</windowclass>
	<windowclass name="reference_weaponstablegroupitem">
		<margins control="0,0,0,2" />
		<sheetdata>
			<anchor_listitem_left_sm />
			<link_listitem_left name="link" />
			<string_listitem_left_noframe_static_linked name="name">
				<anchored width="220" />
			</string_listitem_left_noframe_static_linked>
			<string_listitem_left_noframe_static name="tl">
				<anchored width="50" />
